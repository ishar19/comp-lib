{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "airbnb/base",
        "prettier",
        "plugin:react/recommended",
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "requireConfigfile": false
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "require-jsdoc": 0,
        "prettier/prettier": "error"
    },
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        }
    },
}